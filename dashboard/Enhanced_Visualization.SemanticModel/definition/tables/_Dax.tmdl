	measure Monthly_Sales_Indicate =
			
			VAR _max_profit = CALCULATE(
			    MAXX(
			        VALUES(Superstore[Order Date].[Month]),
			        [Profit]
			    ), ALLSELECTED(Superstore)
			)
			VAR _min_profit = CALCULATE(
			    MINX(
			        VALUES(Superstore[Order Date].[Month]),
			        [Profit]
			    ), ALLSELECTED(Superstore)
			)
			RETURN
			SWITCH(
			    TRUE(),
			    [Profit] = _max_profit, 1,
			    [Profit] = _min_profit, -1,
			    0
			)
		formatString: 0
