	measure Avg_Monthly_Profit =
			
			VAR _totalProfit = [Profit]
			VAR _monthCount = DISTINCTCOUNT('Superstore'[Order Date].[Month])
			RETURN
			    DIVIDE(_totalProfit, _monthCount)
		lineageTag: 0fbb802d-84a5-45ab-967e-b52974a4186f

